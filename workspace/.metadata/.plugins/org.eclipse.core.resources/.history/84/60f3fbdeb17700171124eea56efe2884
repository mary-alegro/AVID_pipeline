package calibration;

public class LensInfo {
	
	private String model;
	private String make;
	private double workDist;
	private double[] FOV = new double[2];
	
	public LensInfo(String m, String mk, double wd, double w, double h) {
		this.model = m;
		this.make = mk;
		this.workDist = w;
		this.FOV[0] = w;
		this.FOV[1] = h;
	}
	
}
